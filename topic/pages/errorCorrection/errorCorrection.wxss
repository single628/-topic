.jiucuo{
    padding:8px 8px;
    position: relative;
}
.text{
    width:100%;
    height:200px;
    border:1px solid silver;
    border-radius: 20px;
}
.emit{
    width:200px;
    height:30px;
    line-height:30px;
    text-align:center;
    font-size:15px;
    color:silver;
    margin:30px auto;
    border:1px solid silver;
    border-radius: 30px;
}
.active-emit{
    width:200px;
    height:30px;
    line-height:30px;
    text-align:center;
    font-size:15px;
    color:white;
    margin:0 auto;
    border:1px solid #1BC7B0;
    border-radius: 30px;
    background-color:#1BC7B0;
}
.place{
    font-size:13px;
    color:silver;
}
.number{
    position:absolute;
    right: 18px;
    top:130px;
    color:silver;
}